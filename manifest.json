{
    "manifest_version": 3,
    "name": "extension",
    "version": "1.0",
    "description": "Various tasks",
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
      ],
    "background": {
        "service_worker": "worker.js"
    },
    "action": {
    "default_popup": "popup/popup.html"
    }
}
